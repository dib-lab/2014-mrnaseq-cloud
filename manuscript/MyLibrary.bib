	Issn = {1526-968X},
	Journal = {Genesis (New York, N.Y. : 2000)},
	Keywords = {Animals, Biological Evolution, Gene Regulatory Networks, Neural Crest, Neural Crest: embryology, Vertebrates, Vertebrates: embryology, Vertebrates: genetics},
	Month = nov,
