	Keywords = {Animals, Biological Evolution, Cell Lineage, Gastrula, Gastrula: embryology, Genome, Neural Plate, Neural Plate: embryology, Urochordata, Urochordata: cytology, Urochordata: embryology, Urochordata: genetics},
	Month = aug,
	Number = {1},
	Pages = {48--60},
	Title = {Unfolding a chordate developmental program, one cell at a time: invariant cell lineages, short-range inductions and evolutionary plasticity in ascidians.},
	Url = {http://www.ncbi.nlm.nih.gov/pubmed/19433085},
