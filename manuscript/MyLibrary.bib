	Keywords = {Animals, Body Patterning, Body Patterning: genetics, Body Patterning: physiology, Bone Morphogenetic Proteins, Bone Morphogenetic Proteins: genetics, Bone Morphogenetic Proteins: metabolism, Ciona intestinalis, Ciona intestinalis: embryology, Ciona intestinalis: genetics, Ciona intestinalis: metabolism, Embryo, Hedgehog Proteins, Hedgehog Proteins: genetics, Hedgehog Proteins: metabolism, In Situ Hybridization, Nonmammalian, Nonmammalian: cytology, Nonmammalian: metabolism, Urochordata, Urochordata: embryology, Urochordata: genetics, Urochordata: metabolism},
