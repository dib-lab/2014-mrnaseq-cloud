	Keywords = {Animals, Biological, Body Patterning, Female, Gastrula, Gastrula: cytology, Larva, Larva: growth \& development, Male, Metamorphosis, Models, Reproduction, Urochordata, Urochordata: growth \& development, Urochordata: physiology, Xenopus, Xenopus: growth \& development, Xenopus: physiology},
	Month = jul,
