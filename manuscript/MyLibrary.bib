	Journal = {Developmental biology},
	Keywords = {Animals, Antisense, Blastomeres, Blastomeres: drug effects, Blastomeres: metabolism, Cell Communication, Cell Communication: physiology, Cell Division, Cell Division: physiology, Embryonic Development, Embryonic Development: physiology, Fetal Proteins, Fetal Proteins: metabolism, Fibroblast Growth Factors, Fibroblast Growth Factors: pharmacology, Oligonucleotides, Organ Specificity, Organ Specificity: physiology, Signal Transduction, Signal Transduction: drug effects, Signal Transduction: physiology, T-Box Domain Proteins, T-Box Domain Proteins: metabolism, Time-Lapse Imaging, Urochordata, Urochordata: embryology},
	Month = jul,
