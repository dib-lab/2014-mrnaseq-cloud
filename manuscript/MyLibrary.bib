@article{de_boever_identification_2008,
	Abstract = {Real-time {PCR} has become a powerful tool for the detection of inflammatory parameters, including cytokines. Reference or housekeeping genes are used for the normalization of real-time {RT}-{PCR} results. In order to obtain reliable results, the stability of these housekeeping genes needs to be determined. In this study the stability of five genes, including beta-actin, glyceraldehyde-3-phosphate dehydrogenase ({GAPDH}), hypoxanthine phophoribosyl-transferase ({HPRT}), ubiquitin ({UB}) and glucose-6-phosphate dehydrogenase (G6PDH), was determined in a lipopolysaccharide inflammation model in chickens. beta-Actin appeared to be the most stable single gene in our model. Because the use of a single gene for normalization can lead to relatively large errors, the use of the geometric mean of multiple reference genes or normalization factor is preferred. The most stable combination for gene expression analysis in this lipopolysaccharide inflammation model in chickens is G6PDH and {UB}, since their correlation coefficients were 0.953 and 0.969, respectively ({BestKeeper}) and an M value of 0.34 and a low V(2/3) value of 0.155 ({geNorm}) were obtained. The use of {HPRT} and {GAPDH} should be avoided. The stable housekeeping genes, G6PDH and {UB} together, can be used to normalize the expression of pro-inflammatory cytokines in a lipopolysaccharide inflammation model in chickens.},
	Author = {De Boever, S and Vangestel, C and De Backer, P and Croubels, S and Sys, S U},
	Doi = {10.1016/j.vetimm.2007.12.002},
	File = {Attachment:/Users/elijahklowe/Library/Application Support/Zotero/Profiles/10oxqg53.default/zotero/storage/GECBPNIM/1-s2.0-S0165242707004278-main.pdf:application/pdf},
	Issn = {0165-2427},
	Journal = {Veterinary immunology and immunopathology},
	Keywords = {Animals, Chickens, Female, Gene Expression Profiling, Gene Expression Regulation, Gene Expression Regulation: physiology, Inflammation, Inflammation: veterinary, Lipopolysaccharides, Lipopolysaccharides: toxicity, Male, Reverse Transcriptase Polymerase Chain Reaction, {RNA}, {RNA}: metabolism},
	Month = apr,
	Number = {3-4},
	Pages = {312--7},
	Title = {Identification and validation of housekeeping genes as internal control for gene expression in an intravenous {LPS} inflammation model in chickens.},
	Url = {http://www.ncbi.nlm.nih.gov/pubmed/18272235},
	Volume = {122},
	Year = {2008},
	Bdsk-Url-1 = {http://www.ncbi.nlm.nih.gov/pubmed/18272235},
