	Journal = {{PLoS} genetics},
	Keywords = {Animals, Chromosome Mapping, Drosophila, Drosophila: genetics, Drosophila: metabolism, Drosophila Proteins, Drosophila Proteins: genetics, Drosophila Proteins: metabolism, Genome, Insect, Insulator Elements, Protein Binding},
