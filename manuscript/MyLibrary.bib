	Issn = {1367-4811},
	Journal = {Bioinformatics (Oxford, England)},
	Keywords = {Algorithms, Binding Sites, Genomics, Genomics: methods, Nucleic Acid, Regulatory Elements, Regulatory Sequences, Sequence Alignment, Transcriptional},
	Month = apr,
	Number = {7},
	Pages = {868--74},
