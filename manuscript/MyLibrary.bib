@article{rahimov_disruption_2008,
	Abstract = {Previously we have shown that nonsyndromic cleft lip with or without cleft palate ({NSCL}/P) is strongly associated with {SNPs} in {IRF}6 (interferon regulatory factor 6). Here, we use multispecies sequence comparisons to identify a common {SNP} (rs642961, G{\textgreater}A) in a newly identified {IRF}6 enhancer. The A allele is significantly overtransmitted (P = 1 x 10(-11)) in families with {NSCL}/P, in particular those with cleft lip but not cleft palate. Further, there is a dosage effect of the A allele, with a relative risk for cleft lip of 1.68 for the {AG} genotype and 2.40 for the {AA} genotype. {EMSA} and {ChIP} assays demonstrate that the risk allele disrupts the binding site of transcription factor {AP}-2alpha and expression analysis in the mouse localizes the enhancer activity to craniofacial and limb structures. Our findings place {IRF}6 and {AP}-2alpha in the same developmental pathway and identify a high-frequency variant in a regulatory element contributing substantially to a common, complex disorder.},
	Author = {Rahimov, Fedik and Marazita, Mary L and Visel, Axel and Cooper, Margaret E and Hitchler, Michael J and Rubini, Michele and Domann, Frederick E and Govil, Manika and Christensen, Kaare and Bille, Camille and Melbye, Mads and Jugessur, Astanand and Lie, Rolv T and Wilcox, Allen J and Fitzpatrick, David R and Green, Eric D and Mossey, Peter a and Little, Julian and Steegers-Theunissen, Regine P and Pennacchio, Len a and Schutte, Brian C and Murray, Jeffrey C},
