	Journal = {Developmental biology},
	Keywords = {Actins, Actins: genetics, Amino Acid, Amino Acid Sequence, Animals, Biological, Biological: genetics, Developmental, Expressed Sequence Tags, Gene Expression Regulation, Larva, Larva: genetics, Larva: metabolism, Metamorphosis, Molecular Sequence Data, Multigene Family, Muscle Proteins, Muscle Proteins: genetics, Notochord, Notochord: metabolism, Phylogeny, Sequence Homology, Urochordata, Urochordata: embryology, Urochordata: genetics, Urochordata: growth \& development},
